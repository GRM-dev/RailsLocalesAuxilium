<local:ProjectPage x:Class="RailsLocalesAuxilium.ProjectPages.ModelPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RailsLocalesAuxilium.ProjectPages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="ModelPage" d:DataContext="{d:DesignInstance local:ModelPage}">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Content="Add New Model" FontSize="16"/>
            <Button Content="Copy translations ..." FontSize="16"/>
        </StackPanel>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="ModelsTree">
            </StackPanel>
        </ScrollViewer>
    </DockPanel>
</local:ProjectPage>
